{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "TicTacToeCSharpApi": {
      "commandName": "Project",
      "commandLineArgs": "api",
      "launchBrowser": true,
      "launchUrl": "api/v1/players",
      "applicationUrl": "http://0.0.0.0:8000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "TicTacToeCSharpPlayerConsumer": {
      "commandName": "Project",
      "commandLineArgs": "worker --queue-name tic-tac-toe-player",
      "launchBrowser": false,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }    
  }
}
